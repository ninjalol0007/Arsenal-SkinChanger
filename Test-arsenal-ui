local Toggle = Tabs.Bubbles:AddToggle("MyToggle", {Title = "Auto Sell", Default = false })

    Toggle:OnChanged(function()
getgenv().collect = true 
while getgenv().collect do
task.wait()
local args = {
	"Sell",
	
}
game:GetService("ReplicatedStorage"):WaitForChild("NetworkRemoteEvent"):FireServer(unpack(args))

end
        print("Toggle changed:", Options.MyToggle.Value)
    end)

    Options.MyToggle:SetValue(false)
