local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()

local Window = Fluent:CreateWindow({
    Title = "BGSL" .. Fluent.Version,
    SubTitle = "1307",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = true, -- The blur may be detectable, setting this to false disables blur entirely
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.LeftControl -- Used when theres no MinimizeKeybind
})
local Tabs = {

    Bubbles = Window:AddTab({ Title = "Bubbles", Icon = "code-xml" }),

    Rewards = Window:AddTab({ Title = "Rewards", Icon = "alarm-clock" }),

    Settings = Window:AddTab({ Title = "Settings", Icon = "settings" })

}

Tabs.Bubbles:AddButton({ Title = "Auto Bubbles", Callback = function()
                 getgenv().farm = true 
while getgenv().farm do
task.wait()
local args = {
	"BlowBubble"
}
game:GetService("ReplicatedStorage"):WaitForChild("NetworkRemoteEvent"):FireServer(unpack(args))
 
 end })
 
 
Tabs.Rewards:AddButton({ Title = "1", Callback = function()
                 getgenv().collect = true 
while getgenv().collect do
task.wait()
local args = {
	"ClaimAchievement",
	1
}
game:GetService("ReplicatedStorage"):WaitForChild("NetworkRemoteEvent"):FireServer(unpack(args))
 end }) 
 
Tabs.Rewards:AddButton({ Title = "2", Callback = function()
                 getgenv().collect = true 
while getgenv().collect do
task.wait()
local args = {
	"ClaimAchievement",
	2
}
game:GetService("ReplicatedStorage"):WaitForChild("NetworkRemoteEvent"):FireServer(unpack(args))
 end }) 

Tabs.Rewards:AddButton({ Title = "3", Callback = function()
                 getgenv().collect = true 
while getgenv().collect do
task.wait()
local args = {
	"ClaimAchievement",
	3
}
game:GetService("ReplicatedStorage"):WaitForChild("NetworkRemoteEvent"):FireServer(unpack(args))
 end }) 

Tabs.Rewards:AddButton({ Title = "4", Callback = function()
                 getgenv().collect = true 
while getgenv().collect do
task.wait()
local args = {
	"ClaimAchievement",
	4
}
game:GetService("ReplicatedStorage"):WaitForChild("NetworkRemoteEvent"):FireServer(unpack(args))
 end }) 
